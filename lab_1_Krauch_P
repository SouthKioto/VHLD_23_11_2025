--process Krauch

library IEEE;
use IEEE.STD_LOGIC_1164.ALL;

entity logic_func is
    Port (
        A : in  std_logic;
        B : in  std_logic;
        C : in  std_logic;
        F : out std_logic
    );
end logic_func;

architecture Behavioral of logic_func is
begin
    process(A, B, C)
    begin
        if A = '1' then
            if B = '1' or C = '1' then
                F <= '1';
            else
                F <= '0';
            end if;
        else
            F <= '0';
        end if;
    end process;
end Behavioral;
