--dataflow

library IEEE;
use IEEE.STD_LOGIC_1164.ALL;

entity logic_func is
    Port (
        A : in  std_logic;
        B : in  std_logic;
        C : in  std_logic;
        F : out std_logic
    );
end logic_func;

architecture Behavioral of logic_func is
begin
    F <= A and (B or C);
end Behavioral;
